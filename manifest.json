
{
  "name": "Webtoon Tracker",
  "description": "Automatically you keeps track of your frequently visited webtoons.",
  "version": "1.1",
  "manifest_version": 3,
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}